#include <iostream>

void stringcopy(char str1[100],char str2[100]);
int main(){
    char s1[100],s2[100];
    printf("Enter first string :");
    gets(s1);
    printf("Enter second string :");
    gets(s2);
    stringcopy(s1,s2);
    printf("string s1 = %s\n",s1);
    return 0;
}

void stringcopy(char str1[100],char str2[100])
{
    unsigned long i;
    i=0;
    while(str2[i] !='\0')
    {
        str1[i] = str2[i];
        i++;
    }
}
    
